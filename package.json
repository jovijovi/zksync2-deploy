{
  "name": "zksync2-deploy",
  "version": "0.1.2",
  "private": false,
  "scripts": {
    "compile": "yarn hardhat compile",
    "deploy": "yarn hardhat deploy-zksync",
    "remixd": "remixd -s ./contracts --remix-ide https://remix.ethereum.org",
    "clean": "yarn hardhat clean ; rm -rf ./dist/* ; rm -rf ./cache",
    "test": "hardhat test",
    "typecheck": "yarn compile && yarn test && yarn tsc --noEmit"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.1.1",
    "@nomiclabs/hardhat-etherscan": "3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@tsconfig/node16": "^1.0.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "6.1.2",
    "@types/chai": "4.3.3",
    "@types/chai-as-promised": "7.1.5",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "chai": "4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.22.0",
    "ethereum-waffle": "3.4.4",
    "hardhat": "^2.10.2",
    "ts-generator": "^0.1.1",
    "ts-node": "10.9.1",
    "typechain": "^8.1.0",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "@matterlabs/hardhat-zksync-deploy": "^0.3.6",
    "@matterlabs/hardhat-zksync-solc": "^0.3.6",
    "@openzeppelin/contracts": "^4.7.3",
    "erc721a": "4.2.2",
